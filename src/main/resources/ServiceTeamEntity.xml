<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="com.terehov.soap.model.ServiceTeamEntity">
        <table name="service_team" schema="public" catalog="soap_service_team_db"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="firstname">
                <column name="firstname" length="30"/>
            </basic>
            <basic name="lastname">
                <column name="lastname" length="30"/>
            </basic>
            <basic name="groupname">
                <column name="groupname" length="30"/>
            </basic>
            <basic name="role">
                <column name="role" length="20"/>
            </basic>
            <basic name="phonenumber">
                <column name="phonenumber"/>
            </basic>
        </attributes>
    </entity>
    <entity class="com.terehov.soap.model.GroupsEntity">
        <table name="groups" schema="public" catalog="soap_service_team_db"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="specializations">
                <column name="specializations" nullable="false"/>
            </basic>
            <basic name="number">
                <column name="number" nullable="false" precision="-1"/>
            </basic>
            <basic name="lecturerId">
                <column name="lecturer_id" nullable="false" precision="-1"/>
            </basic>
            <many-to-one name="lecturersByLecturerId" optional="true"
                         target-entity="com.terehov.soap.model.LecturersEntity">
                <join-column name="lecturer_id" nullable="false" referenced-column-name="id"/>
            </many-to-one>
            <one-to-many name="studentsById" mapped-by="groupsByGroupId"
                         target-entity="com.terehov.soap.model.StudentsEntity"/>
            <one-to-many name="teamsById" mapped-by="groupsByGroupId"
                         target-entity="com.terehov.soap.model.TeamsEntity"/>
        </attributes>
    </entity>
    <entity class="com.terehov.soap.model.LecturersEntity">
        <table name="lecturers" schema="public" catalog="soap_service_team_db"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false"/>
            </basic>
            <basic name="lastName">
                <column name="last_name" nullable="false"/>
            </basic>
            <basic name="urlTg">
                <column name="url_tg" nullable="false"/>
            </basic>
            <one-to-many name="groupsById" mapped-by="lecturersByLecturerId"
                         target-entity="com.terehov.soap.model.GroupsEntity"/>
        </attributes>
    </entity>
    <entity class="com.terehov.soap.model.StudentsEntity">
        <table name="students" schema="public" catalog="soap_service_team_db"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="name">
                <column name="name" nullable="false"/>
            </basic>
            <basic name="lastName">
                <column name="last_name" nullable="false"/>
            </basic>
            <basic name="urlTg">
                <column name="url_tg" nullable="false"/>
            </basic>
            <basic name="groupId">
                <column name="group_id" nullable="false" precision="-1"/>
            </basic>
            <basic name="teamId">
                <column name="team_id" nullable="false" precision="-1"/>
            </basic>
            <many-to-one name="groupsByGroupId" optional="true" target-entity="com.terehov.soap.model.GroupsEntity">
                <join-column name="group_id" nullable="false" referenced-column-name="id"/>
            </many-to-one>
            <many-to-one name="teamsByTeamId" optional="true" target-entity="com.terehov.soap.model.TeamsEntity">
                <join-column name="team_id" nullable="false" referenced-column-name="id"/>
            </many-to-one>
            <one-to-many name="teamsById" mapped-by="studentsByTeamLeaderId"
                         target-entity="com.terehov.soap.model.TeamsEntity"/>
        </attributes>
    </entity>
    <entity class="com.terehov.soap.model.TeamsEntity">
        <table name="teams" schema="public" catalog="soap_service_team_db"/>
        <attributes>
            <id name="id">
                <column name="id" precision="-1"/>
            </id>
            <basic name="teamName">
                <column name="team_name" nullable="false"/>
            </basic>
            <basic name="groupId">
                <column name="group_id" nullable="false" precision="-1"/>
            </basic>
            <basic name="teamLeaderId">
                <column name="team_leader_id" nullable="false" precision="-1"/>
            </basic>
            <many-to-one name="groupsByGroupId" optional="true" target-entity="com.terehov.soap.model.GroupsEntity">
                <join-column name="group_id" nullable="false" referenced-column-name="id"/>
            </many-to-one>
            <many-to-one name="studentsByTeamLeaderId" optional="true"
                         target-entity="com.terehov.soap.model.StudentsEntity">
                <join-column name="team_leader_id" nullable="false" referenced-column-name="id"/>
            </many-to-one>
            <one-to-many name="studentsById" mapped-by="teamsByTeamId"
                         target-entity="com.terehov.soap.model.StudentsEntity"/>
        </attributes>
    </entity>
</entity-mappings>
